@page
@model Milujeme_plarka.Pages.Champions.DeleteModel
@{
    ViewData["Title"] = "Delete";
}

<h2>Odstranění čampa</h2>
<h3>Opravdu chcete čampa odstranit?</h3>
<div>
    <dl>
        <dt>čamp</dt>
        <dd>@Model.Champion.ChampionName</dd>
        <dt>Id</dt>
        <dd>@Model.Champion.ChampionId </dd>
    </dl>

    <form method="post">
        <input type="submit" value="Odstranit" class="btn btn-danger" />
        <a asp-page="./Index" class="btn">Zpět</a>
    </form>
</div>
